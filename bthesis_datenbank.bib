@article{spanet_2,
  abstract = {Reconstructing unstable heavy particles requires sophisticated techniques to sift through the large number of possible permutations for assignment of detector objects to the underlying partons. An approach based on a generalized attention mechanism, symmetry preserving attention networks (SPA-NET), has been previously applied to top quark pair decays at the Large Hadron Collider which produce only hadronic jets. Here we extend the SPA-NET architecture to consider multiple input object types, such as leptons, as well as global event features, such as the missing transverse momentum. In addition, we provide regression and classification outputs to supplement the parton assignment. We explore the performance of the extended capability of SPA-NET in the context of semi-leptonic decays of top quark pairs as well as top quark pairs produced in association with a Higgs boson. We find significant improvements in the power of three representative studies: a search for {\$}{\$}t{$\backslash$}bar{\{}t{\}}H{\$}{\$}, a measurement of the top quark mass, and a search for a heavy {\$}{\$}{\{}Z{\}}\^{}{\{}{\{}{$\backslash$}prime{\}} {\}}{\$}{\$}decaying to top quark pairs. We present ablation studies to provide insight on what the network has learned in each case.},
  author   = {Fenton, Michael James and Shmakov, Alexander and Okawa, Hideki and Li, Yuji and Hsiao, Ko-Yang and Hsu, Shih-Chieh and Whiteson, Daniel and Baldi, Pierre},
  date     = {2024/04/30},
  isbn     = {2399-3650},
  journal  = {Communications Physics},
  number   = {1},
  pages    = {139},
  title    = {Reconstruction of unstable heavy particles using deep symmetry-preserving attention networks},
  volume   = {7},
  year     = {2024}
}


